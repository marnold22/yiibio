{
    "name": "marnold22/yiibio",
    "description": "Whitworth Univesity bioinformatics webtool",
    "keywords": ["yii2", "framework", "advanced", "bio template"],
    "homepage": "http://www.whitworth.edu/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/marnold22/yiibio"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "repositories": [
    {
        "type": "vcs",
        "url": "git@github.com:marnold22/yiibio"
    }
    ],
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
